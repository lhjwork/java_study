package ch02;

public class StackEx02 {
	
	static int sum = 20; // 전역변수
	
	
	static void a() {
		int n1 = 1;//-> 호출안됨 a() 메소드의 stack 영역에 저장됨 (지역변수)
		System.out.println(n1); 
		
	}

	public static void main(String[] args) {
		a();
//		System.out.println(n1); --> 호출 안됨
		System.out.println(sum); 
		
	}

}
